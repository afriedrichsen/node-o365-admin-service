{
  "name": "o365-admin-service",
  "version": "0.1.4",
  "description": "NodeJS application service for Microsoft Azure/Office365 administration.",
  "main": "index.js",
  "scripts": {
    "precommit": "yarn lint",
    "prestart": "yarn docs",
    "start": "cross-env NODE_ENV=production pm2 start ./app/server/index.js",
    "dev": "nodemon ./app/server/index.js",
    "lint": "eslint **/*.js --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:fix": "yarn lint -- --fix",
    "lint:watch": "yarn lint -- --watch",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 20000 --recursive app/server/api/tests",
    "test:unit": "cross-env NODE_ENV=test mocha app/server/tests/unit",
    "test:integration": "cross-env NODE_ENV=test mocha --timeout 20000 app/server/tests/integration",
    "test:watch": "cross-env NODE_ENV=test mocha --watch app/server/tests/unit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "validate": "yarn lint && yarn test",
    "postpublish": "git push --tags",
    "deploy": "sh ./deploy.sh",
    "docs": "apidoc -i app/server -o app/server/docs",
    "server:production": "node app/server/index.js",
    "postdocs": "opn app/server/docs/index.html",
    "docker:start": "cross-env NODE_ENV=production pm2-docker start ./app/server/index.js",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up --abort-on-container-exit"
  },
  "keywords": [
    "Node",
    "Microsoft Graph API",
    "React",
    "Redux"
  ],
  "author": "Alexander Friedrichsen",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.0",
    "bootstrap": "^4.0.0-beta",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "cross-env": "^5.0.1",
    "dotenv-safe": "^5.0.1",
    "express": "^4.15.2",
    "express-validation": "^1.0.2",
    "helmet": "^3.5.0",
    "http-status": "^1.0.1",
    "joi": "^10.6.0",
    "jwt-simple": "0.5.1",
    "lodash": "^4.17.4",
    "method-override": "^2.3.8",
    "moment-timezone": "^0.5.13",
    "mongoose": "^4.9.7",
    "morgan": "^1.8.1",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "4.0.0",
    "pm2": "^2.4.6",
    "q": "^1.5.1",
    "react": "^16.3.2",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "reactstrap": "^5.0.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "simple-oauth2": "^1.5.2",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "apidoc": "^0.17.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.2.0",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^0.14.3",
    "mocha": "^3.3.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.3",
    "opn-cli": "^3.1.0",
    "sinon": "^4.0.1",
    "sinon-chai": "^3.0.0",
    "supertest": "^3.0.0",
    "webpack": "^4.6.0"
  }
}
